// –ü—Ä–∏ –Ω–∞–∂–∞—Ç–∏–∏ –∫–Ω–æ–ø–∫–∏
const handleStartCycle = async () => {
  // –û—Ç–ø—Ä–∞–≤–∫–∞ —Å–æ–±—ã—Ç–∏—è –≤ –∞–Ω–∞–ª–∏—Ç–∏–∫—É
  trackEvent('cycle_start_attempt', {
    user_id: userId,
    queue_position: position,
